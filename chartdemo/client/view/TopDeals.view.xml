<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns="sap.m"
		controllerName="chart_demo.view.TopDeals" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Top 20 Upside Deals"
		showNavButton="{device>/isPhone}" navButtonPress="handleNavButtonPress"
		navButtonTap="onBackButtonPress">
		<content>
			<l:VerticalLayout class="panelBoxContainer sapUiFioriObjectPage"
			width="100%">
			<l:content>
				<Panel>
					<headerToolbar>
						<Toolbar>
							<Label text="Top 20 Upside Deals (Current Total Unweighted KEur)" />
						</Toolbar>
					</headerToolbar>
					<FlexBox id="HorizontalBarChartHolder" alignItems="Start"
						justifyContent="Center"></FlexBox>
				</Panel>
			</l:content>
			</l:VerticalLayout>
		</content>
	</Page>
</core:View>